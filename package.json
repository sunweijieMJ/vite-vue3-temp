{
  "name": "vite-vue3-temp",
  "version": "1.0.0",
  "description": "vite and vue3.0 template",
  "private": true,
  "scripts": {
    "serve": "vite --mode",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build --mode",
    "preview": "vite preview --port 8888",
    "format": "prettier --check .",
    "lint": "npm run lint:script && npm run lint:style && npm run format",
    "lint:script": "eslint src --ext .js,.ts,.jsx,.tsx,.vue",
    "lint:style": "stylelint 'src/**/*.{css,scss,less,vue}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.1.2",
    "element-plus": "^1.0.2-beta.71",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "vue": "^3.2.4",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.0.0",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@intlify/vite-plugin-vue-i18n": "^2.1.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "path-browserify": "^1.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.38.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "4.3.5",
    "vite": "^2.5.0",
    "vite-plugin-compression": "^0.2.4",
    "vite-plugin-style-import": "^0.9.2",
    "vue-tsc": "^0.0.15"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint:script"
    ],
    "src/**/*.{css,scss,less,vue}": [
      "npm run lint:style"
    ]
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.13.4"
  },
  "browserslist": [
    "defaults",
    "> 1%",
    "not ie <= 11",
    "last 2 versions"
  ]
}
