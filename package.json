{
  "name": "vite-vue3-temp",
  "version": "1.0.0",
  "description": "vite and vue3.0 template",
  "private": true,
  "scripts": {
    "preview": "vite preview --port 7000",
    "serve": "vite --mode",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build --mode",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx,.vue src"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.0.2",
    "element-plus": "^1.0.2-beta.41",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "qiankun": "^2.4.0",
    "vue": "^3.0.5",
    "vue-i18n": "^9.1.2",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.0.0-beta.3",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^2.1.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "eslint": "^7.23.0",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "path-browserify": "^1.0.1",
    "sass": "^1.32.8",
    "typescript": "^4.1.3",
    "vite": "^2.2.1",
    "vite-plugin-compression": "^0.2.4",
    "vite-plugin-style-import": "^0.9.2",
    "vue-tsc": "^0.0.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.13.4"
  },
  "browserslist": [
    "defaults",
    "> 1%",
    "not ie <= 11",
    "last 2 versions"
  ]
}
